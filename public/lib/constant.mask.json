{
    "maskType": "Constant",
    "version": "1.0",
    "display": {
        "type": "composite",
        "elements": [
            {
                "type": "rectangle",
                "x": 0,
                "y": 0,
                "width": "100%",
                "height": "100%",
                "fill": "#FFFFFF",
                "stroke": "#D79B00",
                "strokeWidth": 1,
                "rx": 2,
                "useShadow": true
            },
            {
                "type": "text",
                "x": "50%",
                "y": "50%",
                "text": "${value}",
                "fontSize": 12,
                "fontFamily": "Arial, sans-serif",
                "textAnchor": "middle",
                "fill": "#000000",
                "fontWeight": "bold"
            }
        ]
    },
    "parameterBindings": {
        "value": {
            "displayFormat": "number",
            "precision": 3,
            "showUnits": false
        }
    },
    "sizing": {
        "minWidth": 30,
        "minHeight": 30,
        "defaultWidth": 50,
        "defaultHeight": 50,
        "maintainAspectRatio": false
    }
}